{
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "arrow-parens": false,
        "interface-name": false,
        "no-console": false,
        "array-type": false,
        "object-literal-sort-keys": false,
        "member-access": false,
        "space-before-function-paren": false,
        "variable-name": [
            true,
            "check-format",
            "allow-leading-underscore"
        ],
        "max-line-length": [
            true,
            160
        ]
    },
    "rulesDirectory": []
}